<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".greendaotest">

    <TextView
        android:id="@+id/tv_QB_user"
        android:layout_width="348dp"
        android:layout_height="435dp"
        android:layout_marginEnd="20dp"
        android:layout_marginStart="16dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_qb_user" />

    <Button
        android:id="@+id/btn_qb_user"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="83dp"
        android:layout_marginStart="16dp"
        android:text="查询用户信息"
        app:layout_constraintBottom_toTopOf="@+id/tv_QB_user"
        app:layout_constraintEnd_toStartOf="@+id/btn_qb_checkin"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="127dp"
        android:layout_height="18dp"
        android:layout_marginEnd="241dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="30dp"
        android:text="查询用户数据库"
        app:layout_constraintBottom_toTopOf="@+id/btn_qb_checkin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_qb_checkin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="1dp"
        android:layout_marginStart="83dp"
        android:layout_marginTop="1dp"
        android:text="查询签到信息"
        app:layout_constraintBottom_toTopOf="@+id/tv_QB_user"
        app:layout_constraintStart_toEndOf="@+id/btn_qb_user"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

</android.support.constraint.ConstraintLayout>